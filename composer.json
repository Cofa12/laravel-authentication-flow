        "propaganistas/laravel-phone": "^6.0",
        "illuminate/auth": "^12.22"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cofa\\LaravelAuthenticationFlow\\ApiAuthServiceProvider"
            ]
        }
    },
